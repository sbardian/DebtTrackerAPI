{
  "name": "debttrackerapi",
  "version": "2.0.0",
  "description": "API for the DebtTracker App.  Interfaces with MongoDB",
  "main": "index.js",
  "scripts": {
    "start": "node dist/server/index.js",
    "dev": "nodemon dist/server/index.js --session-secret=$DT_SESSION_SECRET --database-url=$DT_DATABASE_URL | bunyan -o short -L",
    "pretest": "npm run clean",
    "test:server": "DT_TEST=true jest",
    "test:server:watch": "DT_TEST=true jest --watch",
    "test:server:coverage": "DT_TEST=true jest --coverage",
    "test:client:run": "cypress run",
    "test:client:open": "cypress open",
    "lint": "eslint . ",
    "build": "npm run build:server && npm run build:client",
    "prebuild": "npm run clean",
    "clean": "rimraf dist",
    "build:server": "babel src/server -d dist/server --source-maps --ignore \"**/*.test.js\"",
    "build:client": "webpack"
  },
  "engines": {
    "node": ">= 6.9.0 < 7.0.0 || >= 8.9.0"
  },
  "author": "Brian Andrews",
  "license": "MIT",
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint",
      "pre-push": "yarn lint"
    }
  },
  "dependencies": {
    "@material-ui/core": "^4.0.0",
    "@material-ui/icons": "^4.0.0",
    "bcrypt": "^3.0.0",
    "body-parser": "^1.17.1",
    "bunyan": "^1.8.12",
    "chart.js": "^2.6.0",
    "classnames": "^2.2.5",
    "connect-history-api-fallback": "^1.4.0",
    "connect-mongo": "^2.0.1",
    "cors": "^2.8.4",
    "css-loader": "^2.0.0",
    "express": "^4.16.2",
    "express-session": "^1.15.6",
    "file-loader": "^4.0.0",
    "financejs": "^4.1.0",
    "jsonwebtoken": "^8.1.0",
    "lodash": "^4.17.11",
    "moment": "^2.18.1",
    "mongoose": "^5.4.20",
    "nodemon": "^1.18.7",
    "prop-types": "^15.6.2",
    "rc-slider": "^8.6.1",
    "rc-tooltip": "^3.4.9",
    "react": "^16.8.0-alpha.0",
    "react-alerts-plus": "^0.0.3",
    "react-dom": "^16.8.0-alpha.0",
    "react-google-charts": "^3.0.0",
    "react-material-ui-form-validator": "^2.0.2",
    "react-router": "^5.0.0",
    "react-router-dom": "^5.0.0",
    "react-router-prop-types": "^1.0.4",
    "style-loader": "^0.23.0",
    "url-loader": "^2.0.0",
    "yargs": "^13.0.0"
  },
  "devDependencies": {
    "@babel/cli": "7.4.4",
    "@babel/core": "7.4.5",
    "@babel/plugin-syntax-dynamic-import": "7.2.0",
    "@babel/plugin-syntax-object-rest-spread": "7.2.0",
    "@babel/preset-env": "7.4.5",
    "@babel/preset-react": "7.0.0",
    "@testing-library/cypress": "4.0.4",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.0.2",
    "babel-jest": "24.8.0",
    "babel-loader": "8.0.6",
    "babel-plugin-transform-class-properties": "6.24.1",
    "coveralls": "3.0.5",
    "cypress": "3.4.0",
    "eslint": "5.16.0",
    "eslint-config-airbnb": "17.1.1",
    "eslint-config-prettier": "4.3.0",
    "eslint-plugin-cypress": "2.2.1",
    "eslint-plugin-import": "2.17.3",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-react": "7.13.0",
    "faker": "4.1.0",
    "html-webpack-plugin": "3.2.0",
    "husky": "3.0.0",
    "jest": "24.8.0",
    "jest-environment-node": "24.8.0",
    "mockingoose": "2.13.1",
    "prettier": "1.18.2",
    "rimraf": "2.6.3",
    "supertest": "4.0.2",
    "supertest-session": "4.0.0",
    "webpack": "4.35.3",
    "webpack-cli": "3.3.4"
  }
}
