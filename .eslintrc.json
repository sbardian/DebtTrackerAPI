{
  "extends": ["airbnb-base", "prettier"],
  "parser": "babel-eslint",
  "rules": {
    "no-unused-vars": "off",
    "valid-jsdoc": [
      "error",
      {
        "prefer": {
          "arg": "param",
          "argument": "param",
          "return": "returns"
        }
      }
    ]
  },
  "plugins": ["react", "jsx-a11y", "import"]
}
